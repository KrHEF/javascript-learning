{
    "compilerOptions": {
      "module": "CommonJS",
      "outDir": "dist/js",
      "removeComments": true,
      "sourceMap":true,
      "strict": true,
      "target": "ESNext",
      "experimentalDecorators": true,
      // "noUnusedLocals": true,
      "useDefineForClassFields": true,
      "strictNullChecks": false,
      "noImplicitReturns": true,
      "noImplicitOverride": true,
    }
}
